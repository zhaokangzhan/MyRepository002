<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<!-- 建立类与表关系映射 -->
	<class name="domain.Test" table="test" >
		<!-- 建立类中的属性与表中的主键对应 -->
		<id name="id" column="Id">
			<generator class="native"></generator>
		</id>
		<!-- 建立类中的普通的属性和表的字段的对应 -->
		
		<property name="test_uuid" column="test_uuid"  />
		<property name="test_question" column="test_question" />
		<property name="test_select_a" column="test_select_a" />
		<property name="test_select_b" column="test_select_b" />
		<property name="test_select_c" column="test_select_c" />
        <property name="test_select_d" column="test_select_d" />
         <property name="test_answer" column="test_answer" />
           <property name="test_explain" column="test_explain" />

	</class>
	
	
</hibernate-mapping>