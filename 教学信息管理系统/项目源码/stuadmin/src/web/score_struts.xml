<struts>
	<!-- Struts2为了管理Action的配置，通过包进行管理。 -->
	<!-- 配置Struts2的包 ================ -->
	
	<package name="score" extends="struts-default" namespace="/">
		<!-- 配置Action================ -->
		<action name="score_*" class="scoreAction" method="{1}">
		
		 <result name="touser_test"  type="dispatcher">/user_jsp/admin/score_charts.jsp</result> 
		
		</action>
	
		
	</package>
	
		
</struts>
