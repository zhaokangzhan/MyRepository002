<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!-- Struts2为了管理Action的配置，通过包进行管理。 -->
	<!-- 配置Struts2的包 ================ -->
	
	<package name="test" extends="struts-default" namespace="/">
		<!-- 配置Action================ -->
		<action name="t_*" class="testAction" method="{1}">
			<!-- <result name="tophysics"  type="dispatcher">/jsp/test/physics.jsp</result> -->
		<!-- <result name="totestscore"  type="dispatcher">/jsp/test/testscore.jsp</result> -->
		<result name="tophysics"  type="dispatcher">/physics.jsp</result>
		<result name="totestscore"  type="dispatcher">/testscore.jsp</result>
		</action>
	
		
	</package>
	
		
</struts>
